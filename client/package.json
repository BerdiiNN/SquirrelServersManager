{
  "name": "ant-design-pro",
  "version": "0.1.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "./node_modules/.bin/cross-env ANALYZE=1 max build",
    "build": "./node_modules/.bin/max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
     "i18n-remove": "./node_modules/.bin/pro i18n-remove --locale=zh-CN --write",
    "postinstall": "./node_modules/.bin/max setup",
    "jest": "./node_modules/.bin/jest",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "./node_modules/.bin/lint-staged",
    "lint-staged:js": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "./node_modules/.bin/eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "./node_modules/.bin/eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "./node_modules/.bin/prettier -c --write \\\"**/**.{js,jsx,tsx,ts,less,md,json}\\\" --end-of-line auto",
    "openapi": "./node_modules/.bin/max openapi",
    "prettier": "./node_modules/.bin/prettier -c --write \\\"**/**.{js,jsx,tsx,ts,less,md,json}\\\"",
    "preview": "npm run build && ../node_modules/.bin/max preview --port 8000",
    "record": "./node_modules/.bin/cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "./node_modules/.bin/cross-env UMI_ENV=dev max dev",
    "start:dev": "./node_modules/.bin/cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "./node_modules/.bin/cross-env MOCK=none UMI_ENV=dev max dev",
    "start:pre": "./node_modules/.bin/cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "./node_modules/.bin/cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "test": "./node_modules/.bin/jest",
    "test:coverage": "npm run jest -- --coverage",
    "test:update": "npm run jest -- -u",
    "tsc": "./node_modules/.bin/tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.43",
    "@ant-design/use-emotion-css": "1.0.4",
    "@umijs/route-utils":"^4.0.1",
    "@ant-design/charts": "^2.0.3",
    "antd": "^5.12.8",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "omit.js": "^2.0.2",
    "querystring": "^0.2.1",
    "rc-menu": "^9.12.0",
    "rc-util": "^5.37.0",
    "react": "^18.2.0",
    "react-dev-inspector": "^1.9.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-terminal": "^1.4.4",
    "@monaco-editor/react": "^4.6.0"
  },
  "devDependencies": {
    "antd-pro-merge-less": "^3.0.11",
    "@ant-design/pro-cli": "^3.1.0",
    "@ant-design/plots": "^2.1.6",
    "@testing-library/react": "^13.4.0",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.18",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/luxon": "^3.3.5",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "@types/react-helmet": "^6.1.7",
    "@umijs/fabric": "^4.0.1",
    "@umijs/lint": "^4.1.0",
    "@umijs/max": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.8",
    "swagger-ui-dist": "^4.19.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "umi-presets-pro": "^2.0.3"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
